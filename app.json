{
  "expo": {
    "name": "Saraya",
    "slug": "sarayaplus",
    "scheme": "sarayaplus",
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./App/assets/sateh.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./App/assets/sateh.png",
      "resizeMode": "contain",
